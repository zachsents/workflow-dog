{
    "compilerOptions": {
        "moduleResolution": "Node",
        "jsx": "react-jsx",
        "baseUrl": ".",
        "paths": {
            "@web/*": [
                "../web/*"
            ],
            "@types": [
                "./types.js"
            ],
            "@/*": [
                "./*"
            ]
        },
        "esModuleInterop": true,
        "target": "ESNext",
        "module": "ESNext",
        "declaration": true,
        "declarationMap": true,
        "composite": true,
        "preserveSymlinks": true
    }
}